local Aurora = loadstring(game:HttpGet("YOUR_SCRIPT_URL"))()

local window = Aurora:CreateWindow({
    Title = "Premium UI",
    ToggleKey = Enum.KeyCode.RightShift
})

local mainTab = window:CreateTab("Main")
local visualsSection = mainTab:CreateSection("Visuals")

visualsSection:AddLabel("Premium Settings")
visualsSection:AddToggle({
    Label = "Enable Effects",
    Default = true,
    Callback = function(state) print("Effects:", state) end
})

visualsSection:AddSlider({
    Label = "Quality",
    Min = 0,
    Max = 100,
    Default = 75,
    Callback = function(value) print("Quality:", value) end
})

visualsSection:AddButton({
    Text = "Apply Settings",
    Callback = function() Aurora:Notify({
        Title = "Success",
        Message = "Settings applied successfully",
        Type = "success",
        Duration = 3
    }) end
})
